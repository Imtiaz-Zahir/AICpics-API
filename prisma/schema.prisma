generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model photos {
  id           String      @id @default(auto()) @map("_id") @db.ObjectId
  v            Int         @map("__v")
  attachmentID String
  build_At     DateTime    @db.Date
  channelID    String
  createdAt    DateTime    @db.Date
  download     Int
  expiresAt    String
  fileName     String
  height       Int
  issuedAt     String
  massageID    String      @unique(map: "massageID_1")
  prompt       String
  signature    String
  size         Int
  updatedAt    DateTime    @db.Date
  width        Int
  favorites    favorites[]
}

model users {
  email     String      @id @map("_id")
  name      String
  image     String
  lastVisit DateTime    @default(now())
  joinAt    DateTime    @default(now())
  favorites favorites[]
}

model favorites {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  email     String
  users     users    @relation(fields: [email], references: [email])
  photoID   String   @db.ObjectId
  photos    photos   @relation(fields: [photoID], references: [id])
  createdAt DateTime @default(now())

  @@unique([email, photoID])
  @@index([email])
}

model feedbacks {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  email   String
  message String
}
